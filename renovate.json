{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
    ":semanticCommits",
    ":dependencyDashboard"
 ],
  
  
  "packageRules": [
    {
      "matchPackagePatterns":["*"],
      "groupName":"other packages"
    },
    {
      "matchPackagePrefixes":["ArmoniK"],
      "groupName":"ArmoniK"
    },
    {
      "matchPackagePrefixes":["ArmoniK.Api"],
      "groupName":"ArmoniK.Api"
    },
    {
    "matchPaths": ["**.csproj"],
     "groupName": "all nuget"
    }
    
    
  ]
}
