{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
    ":semanticCommits",
    ":dependencyDashboard"
 ],
  
  
  "packageRules": [
    {
      "matchPackagePatterns":["*"],
      "groupName":"other packages"
    },
    {
      "matchPackagePrefixes":["ArmoniK.Api"],
      "groupName":"ArmoniK.Api"
    },
    {
      "matchPackagePrefixes":["ArmoniK"],
      "groupName":"ArmoniK",
      "excludePackgePrefixes":["ArmoniK.Api"]
    }
  ]
}
